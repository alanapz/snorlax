<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="snorlax-default" type="log4net.Appender.ConsoleAppender">
    <threshold>INFO</threshold>
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%20date{dd/MM/yy HH:mm:ss,fff} | %-5level - %message%newline" />
		</layout>
	</appender>
	<root>
		<level value="DEBUG" />
    <appender-ref ref="snorlax-default" />
	</root>
</log4net>